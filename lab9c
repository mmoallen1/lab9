#include <cstdlib>
#include <iostream>

using namespace std;

int main()
{
    int i=0;
    int evencount=0;
    int oddcount=0;
    int array1[15];
    for (i=0; i<15; i++){
        array1[i]=rand()%20;
        cout<<array1[i]<<endl;

    }

    for (i=0; i<15; i++){
        if (array1[i] % 2==0)
            evencount++;
        else
        oddcount++;
    }
 cout<<"number of even: "<<evencount<<endl;
 cout<<"number of odd: "<<oddcount<<endl;

    return 0;
}
