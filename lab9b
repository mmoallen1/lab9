#include <iostream>

using namespace std;

int main()
{
    int i=0;
    int num=0;
    int array1[10];
    int array2[10];
    for (i=0; i<10; i++){
        cout<<"Enter number "<<i+1<<endl;
        cin>>num;
        array1[i]=num;

    }
     cout<<"Array 1: "<<endl;
     for (i=0; i<10; i++){

            cout<<" " << array1[i];

    }
    cout<<"\nArray 2: "<<endl;
    for (i=9; i>=0; i--){
        cout<<" " <<array1[i];

    }
    return 0;
}
